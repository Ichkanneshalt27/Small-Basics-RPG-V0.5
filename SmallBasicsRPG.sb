Sub Ki
  Attack = 1
  Stats["check"] = Stats["MaxHp"] * 0.40
  If Stats["Hp"] < Stats["check"] Then
    Roll = Math.GetRandomNumber(8)
    If Roll = 1 Then
      Monster["Angriff2"] = Monster["Angriff2"] - 1
    ElseIf Roll = 2 then
      Attack = 0
      Mheal = Math.GetRandomNumber(4)
      Monster["Warten"] = Monster["Angriff2"] + Mheal
    ElseIf Roll = 3 then  
      Monster["Angriff"] = Monster["Angriff2"]
    ElseIf Roll = 4 then  
      Monster["Angriff"] = Monster["Angriff2"] - 1
    ElseIf  Roll = 5 then 
      Monster["Angriff"] = Monster["Angriff2"] + 1
    ElseIf Roll = 6Then  
      Monster["Angriff"] = Monster["Angriff2"]
    ElseIf Roll = 7 then  
      Monster["Angriff"] = Monster["Angriff2"] + 1
    ElseIf Roll = 8 Then  
      Monster["Angriff"] = Monster["Angriff2"]
    EndIf
  ElseIf Monster["Hp"] = Monster["MaxHp"] then
    Roll = Math.GetRandomNumber(8)
    If Roll = 1 Then
      Monster["Angriff2"] = Monster["Angriff2"] - 1
    ElseIf Roll = 2 then
      Attack = -1
      Monster["Block"] = Monster["Angriff2"]
    ElseIf Roll = 3 then  
      Monster["Angriff"] = Monster["Angriff2"]
    ElseIf Roll = 4 then  
      Monster["Angriff"] = Monster["Angriff2"] - 1
    ElseIf  Roll = 5 then 
      Monster["Angriff"] = Monster["Angriff2"] + 1
    ElseIf Roll = 6Then  
      Monster["Angriff"] = Monster["Angriff2"]
    ElseIf Roll = 7 then  
      Monster["Angriff"] = Monster["Angriff2"] + 1
    ElseIf Roll = 8 Then  
      Monster["Angriff"] = Monster["Angriff2"]
    EndIf
  Else
    Roll = Math.GetRandomNumber(8)
    If Roll = 1 Then
      Monster["Angriff"] = Monster["Angriff2"] - 1
    ElseIf Roll = 2 then
      Attack = 0
      Mheal = Math.GetRandomNumber(3)
      Monster["Warten"] = Monster["Angriff"] + Mheal
    ElseIf Roll = 3 then  
      Attack = -1
      Monster["Block"] = Monster["Angriff2"]
    ElseIf Roll = 4 then  
      Monster["Angriff"] = Monster["Angriff2"]
    ElseIf  Roll = 5 then 
      Monster["Angriff"] = Monster["Angriff2"] + 1
    ElseIf Roll = 6 then  
      Monster["Angriff"] = Monster["Angriff2"] + 2
    Elseif Roll = 7 Then  
      Monster["Angriff"] = Monster["Angriff2"]
    Elseif Roll = 8 Then 
      Monster["Angriff"] = Monster["Angriff2"]
    EndIf
  EndIf
EndSub

